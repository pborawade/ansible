{
    
"AWSTemplateFormatVersion" : "2010-09-09",



"Parameters": {
    
   "PublicAZ":
   { 
    "Description" :"PublicAZ",
    "Type": "AWS::EC2::AvailabilityZone::Name",
    "Default":"-"
  }
    
      },


"Resources": {
    
    "myvpc": {
      "Type": "AWS::EC2::VPC",
      "Properties": {
        "CidrBlock": "10.10.0.0/16"
      }
    },
    
    
    
    "Publicsubnet": {
      "Type": "AWS::EC2::Subnet",
      "Properties": {
        "AvailabilityZone": 
        {
           "Ref": "PublicAZ" },
    
        "VpcId":
        { "Ref":"myvpc"},

        "CidrBlock": "10.10.1.0/24"
        }
      },
      
      "Publicroutetable": {
        "Type": "AWS::EC2::RouteTable",
        "Properties": {
          "VpcId": {
           "Ref": "myvpc" 
          }
        }
      }
      

    
    
 },

"Outputs" : {}




}